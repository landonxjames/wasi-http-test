{
  "name": "wasmVersionTests",
  "version": "0.0.1",
  "description": "wasmVersionTests ACT Component",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "require": "./component.js",
      "import": "./component.js",
      "default": "./component.js"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:rust && npm run build:wasm-component && npm run copy:wasm-component && npm run copy:package.json && npm run build:wasm-js-bindings",
    "build:rust": "cargo build --target=wasm32-wasi --release --manifest-path ./runtime/Cargo.toml",
    "build:wasm-component": "wasm-tools component new ./runtime/target/wasm32-wasi/release/wasi_http_tests.wasm -o ./runtime/target/wasm32-wasi/release/wasi_http_tests_component.wasm --adapt ./runtime/wasi_snapshot_preview1.reactor.wasm",
    "build:wasm-js-bindings": "npx jco transpile ./dist/wasm/wasi_http_tests_component.wasm -o ./src/generated-js-runtime-bindings --no-namespaced-exports",
    "copy:wasm-component": "run-script-os",
    "copy:wasm-component:windows": "mkdir -force \"./dist/wasm/\" && cp -R ./runtime/target/wasm32-wasi/release/wasi_http_tests_component.wasm ./dist/wasm/",
    "copy:wasm-component:default": "mkdir -p ./dist/wasm/ && cp -R ./runtime/target/wasm32-wasi/release/wasi_http_tests_component.wasm ./dist/wasm/",
    "copy:package.json": "cp ./package.json ./dist/",
    "test": "vitest --run",
    "test:py": "python3 -m pytest src/test_component_no_resources.py",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@bytecodealliance/jco": "^0.14.2",
    "@bytecodealliance/preview2-shim": "^0.14.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.7.18",
    "aws-cdk": "^2.94.0",
    "cross-env": "^7.0.3",
    "execa": "^8.0.1",
    "run-script-os": "^1.1.6",
    "tsx": "^4.7.0",
    "typescript": "=5.1.3",
    "vitest": "^1.1.0"
  }
}
